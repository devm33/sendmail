{
    // Globals
    "undef":        true,
    "node":         true,
    // Whitespace
    "white":        true, // the crockford rule
    "trailing":     true,
    "indent":       4,
    "smarttabs":    false, // no tabs
    // Other Styles
    //"eqeqeq":       true, // ===
    "quotmark":     true, // consistency, could specify single or double here
    "curly":        true, // requires {} always
    "unused":       true // warn about unused vars
}
